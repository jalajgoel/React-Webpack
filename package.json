{
  "name": "react-webpack",
  "version": "1.0.0",
  "description": "Its just a react and webpack setup",
  "main": "index.js",
  "scripts": {
    "start": "npm run dev",
    "build:dev": "./node_modules/.bin/webpack -d",
    "dev": "webpack-dev-server -d --inline --hot",
    "dev:open": "webpack-dev-server -d --open",
    "build:prod": "./node_modules/.bin/webpack -p",
    "prod": " nodemon ./node_modules/.bin/webpack -p --hot"
